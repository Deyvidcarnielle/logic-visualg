algoritmo "semnome"
// Função :
// Autor :
// Data : 27/07/2023
// Seção de Declarações
var
h, pesoh, pesom: real


inicio
// Seção de Comandos

escreva("Insira sua altura: ")
leia(h)

pesoh <- (72.7*h) -58
pesom <- (62.1*h) -44.7

escreval("O peso ideal para homens é: ", pesoh:2:2, " Kg")
escreval("O peso ideal para mulheres é: ", pesom:2:2, " Kg")

fimalgoritmo
